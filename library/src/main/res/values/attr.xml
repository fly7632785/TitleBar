<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TitleBar">
        <attr name="tb_left_img_type" format="enum">
            <enum name="none" value="0" />
            <enum name="back" value="1" />
            <enum name="menu" value="2" />
            <enum name="close" value="3" />
        </attr>
        <attr name="tb_left_img" format="reference"/>
        <attr name="tb_left_text" format="string"/>
        <attr name="tb_left_text_left_drawable" format="reference"/>
        <attr name="tb_center_text" format="string"/>
        <attr name="tb_right_text" format="string"/>
        <attr name="tb_right_img1" format="reference"/>
        <attr name="tb_right_img2" format="reference"/>

    </declare-styleable>
</resources>